{
  "name": "uoc-boilerplate",
  "version": "3.7.0",
  "private": true,
  "description": "Boilerplate for Advanced HTML/CSS Tools UOC students",
  "scripts": {
    "parcel:serve": "parcel src/index.html -p 8123 --target web --open",
    "parcel:build": "parcel build src/index.html --target web --no-source-maps --no-cache",
    "clean": "rimraf dist .cache .cache-loader .parcel-cache",
    "dev": "npm-run-all clean parcel:serve",
    "build": "npm-run-all clean parcel:build",
    "test": "echo 'Everything is working as expected ✅\nStart working on your project by running \\033[1mnpm run dev\\033[0m'"
  },
  "web": "dist/index.html",
  "targets": {
    "web": {
      "context": "browser",
      "publicUrl": "./",
      "engines": {
        "browsers": "last 2 versions, > 0.5%, not dead"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmhr0/diw_sitio_Rogelio_Villaverde.git"
  },
  "author1": {
    "name": "Juan Manuel Herrera Ramirez",
    "email": "jherram0603@g.educaand.es"
  },
  "author2": {
    "name": "Antonio Jesús Téllez Perdigones",
    "email": "atelper1005@g.educaand.es"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmhr0/diw_sitio_Rogelio_Villaverde"
  },
  "homepage": "https://github.com/jmhr0/diw_sitio_Rogelio_Villaverde#readme",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.10.3",
    "autoprefixer": "^10.4.15",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.10.3",
    "postcss-preset-env": "^9.1.1",
    "posthtml-include": "^1.7.4",
    "rimraf": "^5.0.1",
    "sharp": "0.31.1"
  },
  "dependencies": {
    "sass": "^1.69.5"
  }
}
